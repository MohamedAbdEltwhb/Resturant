<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.SignUpActivity"
    android:background="@drawable/loginbg">

    <ImageButton
        android:id="@+id/image_button_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:background="#00000000"
        android:src="@drawable/ic_arrow_back_black_24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="36dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="88dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <com.example.mm.resturant.customfonts.MyTextView_Roboto_Bold
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="RESTURANT"
            android:textColor="#ffffff"
            android:textSize="31sp" />

        <View
            android:layout_width="1dp"
            android:layout_height="34dp"
            android:layout_marginLeft="9.2dp"
            android:layout_marginTop="5dp"
            android:background="#ffffff" />

        <com.example.mm.resturant.customfonts.MyTextView_Roboto_Regular
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="8.5dp"
            android:layout_marginStart="8.5dp"
            android:text="SIGN UP"
            android:textColor="#f05421"
            android:textSize="20sp" />
    </LinearLayout>

    <com.example.mm.resturant.customfonts.EditText_Roboto_Regular
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/f-name_id"
        style="@style/editTextRegister"
        android:layout_marginLeft="34.6dp"
        android:layout_marginStart="34.6dp"
        android:layout_marginRight="33.6dp"
        android:layout_marginEnd="33.6dp"
        android:layout_marginTop="54.7dp"
        android:hint="F - name"
        android:inputType="textPersonName"
        android:drawableEnd="@drawable/ic_profile"
        android:drawableRight="@drawable/ic_profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="34.6dp"
        android:layout_marginStart="34.6dp"
        android:layout_marginRight="33.6dp"
        android:layout_marginEnd="33.6dp"
        android:layout_marginTop="6.5dp"
        android:background="#33ffffff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/f-name_id" />

    <com.example.mm.resturant.customfonts.EditText_Roboto_Regular
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/l-name_id"
        style="@style/editTextRegister"
        android:layout_marginEnd="33.6dp"
        android:layout_marginLeft="34.6dp"
        android:layout_marginRight="33.6dp"
        android:layout_marginStart="34.6dp"
        android:layout_marginTop="16dp"
        android:hint="L - name"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/f-name_id" />

    <View
        android:id="@+id/view1"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="34.6dp"
        android:layout_marginStart="34.6dp"
        android:layout_marginRight="33.6dp"
        android:layout_marginEnd="33.6dp"
        android:layout_marginTop="6.5dp"
        android:background="#33ffffff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/l-name_id" />

    <com.example.mm.resturant.customfonts.EditText_Roboto_Regular
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/email_id"
        style="@style/editTextRegister"
        android:layout_marginEnd="33.6dp"
        android:layout_marginLeft="34.6dp"
        android:layout_marginRight="33.6dp"
        android:layout_marginStart="34.6dp"
        android:layout_marginTop="16dp"
        android:hint="Email"
        android:inputType="textEmailAddress"
        android:drawableEnd="@drawable/ic_email"
        android:drawableRight="@drawable/ic_email"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view1" />

    <View
        android:id="@+id/view2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="34.6dp"
        android:layout_marginStart="34.6dp"
        android:layout_marginRight="33.6dp"
        android:layout_marginEnd="33.6dp"
        android:layout_marginTop="6.5dp"
        android:background="#33ffffff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email_id" />

    <com.example.mm.resturant.customfonts.EditText_Roboto_Regular
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/password_id"
        style="@style/editTextRegister"
        android:layout_marginEnd="33.6dp"
        android:layout_marginLeft="34.6dp"
        android:layout_marginRight="33.6dp"
        android:layout_marginStart="34.6dp"
        android:layout_marginTop="16dp"
        android:hint="Password"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2" />

    <View
        android:id="@+id/view3"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="34.6dp"
        android:layout_marginStart="34.6dp"
        android:layout_marginRight="33.6dp"
        android:layout_marginEnd="33.6dp"
        android:layout_marginTop="6.5dp"
        android:background="#33ffffff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/password_id" />

    <com.example.mm.resturant.customfonts.EditText_Roboto_Regular
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/re_password_id"
        style="@style/editTextRegister"
        android:layout_marginEnd="33.6dp"
        android:layout_marginLeft="34.6dp"
        android:layout_marginRight="33.6dp"
        android:layout_marginStart="34.6dp"
        android:layout_marginTop="16dp"
        android:hint="Re-Password"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view3" />

    <View
        android:id="@+id/view4"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="34.6dp"
        android:layout_marginStart="34.6dp"
        android:layout_marginRight="33.6dp"
        android:layout_marginEnd="33.6dp"
        android:layout_marginTop="6.5dp"
        android:background="#33ffffff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/re_password_id" />

    <com.example.mm.resturant.customfonts.EditText_Roboto_Regular
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/phone_id"
        style="@style/editTextRegister"
        android:layout_marginEnd="33.6dp"
        android:layout_marginLeft="34.6dp"
        android:layout_marginRight="33.6dp"
        android:layout_marginStart="34.6dp"
        android:layout_marginTop="16dp"
        android:hint="PhoneNumber"
        android:inputType="phone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view4" />

    <View
        android:id="@+id/view5"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="34.6dp"
        android:layout_marginStart="34.6dp"
        android:layout_marginRight="33.6dp"
        android:layout_marginEnd="33.6dp"
        android:layout_marginTop="6.5dp"
        android:background="#33ffffff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/phone_id" />

    <com.example.mm.resturant.customfonts.MyTextView_Roboto_Regular
        android:id="@+id/sign_Un_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="31dp"
        android:layout_marginEnd="33.6dp"
        android:layout_marginLeft="34.6dp"
        android:layout_marginRight="33.6dp"
        android:layout_marginStart="34.6dp"
        android:layout_marginTop="53.8dp"
        android:background="@drawable/rectangle"
        android:gravity="center_horizontal"
        android:padding="16.4dp"
        android:text="SIGN UP"
        android:textColor="#ffffff"
        android:textSize="13.3sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view5" />

</android.support.constraint.ConstraintLayout>